{
    "name": "Facebook ads removal",
    "version": "0.4",
    "minimum_chrome_version": "49.0",
    "description": "Just remove Facebook ads",
	"content_scripts": [{
		"matches": ["https://www.facebook.com/*"],
		"js": ["content-script.js"],
        "run_at": "document_end"
	}],
    "manifest_version": 2
}
